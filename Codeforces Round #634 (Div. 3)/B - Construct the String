
#include<bits/stdc++.h>
using namespace std;
#define happy ios::sync_with_stdio(false);
#define coding  cin.tie(0);

char ch[26] = {'a','b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z'};

int solve() 
{
	//happy coding

	int n,a,b;
	cin>>n>>a>>b;
	for(int i=0;i<n;i++){
		cout<<ch[i%b];
	}cout<<endl;

	return 0;
}
int main(){
	int test = 1;
	scanf("%d", &test);
	while (test--)solve();return 0;
}
